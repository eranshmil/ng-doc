{
	"name": "ng-doc-add",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/ng-doc-add",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/ng-doc-add",
				"main": "libs/ng-doc-add/schematics/index.ts",
				"tsConfig": "libs/ng-doc-add/tsconfig.lib.json",
				"assets": [
					"libs/ng-doc-add/*.md",
					"libs/ng-doc-add/schematics/**/*.json",
					"libs/ng-doc-add/schematics/**/files/**/*"
				],
				"updateBuildableProjectDepsInPackageJson": false
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["libs/ng-doc-add/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/ng-doc-add/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
